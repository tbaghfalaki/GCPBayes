Package: GCPBayes
Type: Package
Title: Bayesian Meta-Analysis of Pleiotropic Effects Using Group Structure
Version: 4.2.0
Authors@R: c(
    person("Yazdan", "Asgari", email = "yazdan1130@gmail.com", role = c("aut", "cre")),
	person("Taban", "Baghfalaki", role = c("cre", "aut")),
	person("Benoit", "Liquet", role = "aut"),
	person("Pierre-Emmanuel", "Sugier", role = "aut"),
	person("Mohammed", "Sedki", role = "aut"),
	person("Therese", "Truong", role = "aut")
